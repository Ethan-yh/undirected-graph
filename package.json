{
    "name": "yh-graph",
    "version": "1.0.0",
    "description": "An graph application",
    "main": "main.js",
    "build": {
        "appId": "com.graph.app",
        "mac": {
            "target": [
                "dmg",
                "zip"
            ]
        },
        "win": {
            "target": [
                "nsis",
                "zip"
            ],
            "icon": "./static/images/udg.ico"
        }
    },
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder --win --x64"
    },
    "author": "Ethan-yh",
    "devDependencies": {
        "electron": "^9.1.0",
        "electron-builder": "^22.7.0"
    }
}